		<script type="text/javascript">
			function fnOnUpdateValidators() {
				for (var i = 0; i < Page_Validators.length; i++) {
					var val = Page_Validators[i];
					var ctrl = document.getElementById(val.controltovalidate);
	
					if (i > 0) {
						if (ctrl != null && ctrl.style != null) {
							if (!val.isvalid) {
								ctrl.style.background = '#f8dbdf';
								ctrl.style.border = '1px solid #e13433';
	
							}
							else {
								if (ctrl != prevControl) {
									ctrl.style.backgroundColor = '';
									ctrl.style.border = 'none';
								}
							}
						}
					}
					else {
						if (ctrl != null && ctrl.style != null) {
							if (!val.isvalid) {
								ctrl.style.background = '#f8dbdf';
								ctrl.style.border = '1px solid #e13433';
	
							}
							else {
								ctrl.style.backgroundColor = '';
								ctrl.style.border = 'none';
							}
						}
					}
					prevControl = document.getElementById(val.controltovalidate);
				}
			}
			
			var qouteSubmitted = false;
			
			function toggleRequestQuote() {
				if ($(window).width() > 600) {
					if(qouteSubmitted == false) {
						if($(".request-tab").attr("src") == "/images/request-quote-open.png") {
							$(".request-tab").attr("src", "/images/request-quote-close.png");
							$(".sticky-form").animate({bottom: "0"}, 600);
						}
						else {
							$(".request-tab").attr("src", "/images/request-quote-open.png");
							$(".sticky-form").animate({bottom: "-425px"}, 600);
						}
					}
					else {
						$(".request-tab").attr("src", "/images/request-quote-open.png");
						$(".sticky-form").animate({bottom: "-425px"}, 600);
						$(".sticky-form").hide();
					}
				}
				else {
					window.location.href = "/request-a-quote/";
					return false;
				}
			 }
			 
			 function toggleThanksQuote() {
			 	$("#quote_form").hide();
				$("#quote_thanks").show();
				$(".request-tab").attr("src", "/images/request-quote-close.png");
				$(".request-tab").animate({left: "147px"}, 600);
			 	$(".sticky-form").animate({bottom: "0"}, 600);
				
				qouteSubmitted = true;
			 }
		</script>

