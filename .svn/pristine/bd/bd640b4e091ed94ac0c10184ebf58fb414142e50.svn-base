/*Segment Bar */

.segment_fixed
{
    position:fixed; 
    top:115px; 
    margin:0 auto; 
    width:100%;
    z-index:3500;
}

.segment_body
{
    position:relative; 
    /*top:68px; 
    padding-bottom:70px; */
}

.segment_bar
{
    font-family:'futura medium condensed', Arial, Sans-Serif;
    font-size:40px;
    line-height:50px;
    color:#ffffff;
    padding-top:10px;
}

.segment_slant
{
    height:59px; 
    vertical-align:bottom;
}

.segment_slants
{
    width:155px;
}    

.segment_slant_td, .segment_slant_td2
{
    width:155px;
}

.icon
{
    width:50px;
}

.segment_title
{
    font-family:'futura medium condensed', Arial, Sans-Serif;
    font-size:41px;
    line-height:41px;
    color:#c3c3c3;
}

.back_arrow
{
    position:relative;
    left:120px;
    top:-20px;
}

.segment_bestinclass
{
    position:absolute; 
    top:20px; 
    left:40px;
}

@media screen and (max-width: 900px) 
{
    .segment_fixed
    {
        top:52px;
    }
}

@media all and (max-width:850px) 
{
    .segment_slant_td
    {
        width:100px;
    }
    .segment_slant
    {
        height:40px;
    }
}

/* Mobile Segment Bar- Not Fixed Position and no slants */
    @media all and (max-width:768px) 
    {
        .segment_fixed
        {
            position:absolute; 
        }

        .segment_slant_td
        {
            width:30px;
        }
        
        .segment_slant_td2
        {
            width:0px;
        }
        
        .segment_slant_td .segment_slants
        {
            width:50px;
        }
        
        .segment_slant_td2 .segment_slants
        {
            width:0px;
        }
        
        .back_arrow
        {
            left:15px;
        }
    }
    
    @media all and (max-width:800px) 
    {
        .segment_bar
        {
            padding-bottom:10px;
            padding-top:15px;
        }
    }
    
    @media all and (max-width:470px) 
    {
        .segment_bar
        {
            font-size:27px;
            line-height:30px;
        }
        
        .segment_slants
        {
            width:35px;
        }
        
        .icon
        {
            width:35px;
            padding-right:8px;
        }
        
        .segment_bestinclass
        {
            width:70px;
            top:10px; 
            left:0px;
        }
    }

