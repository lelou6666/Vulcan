/* CSS Document */

body
{
	background-color:#404040;
}

* {
	margin:0px;
	padding:0px;
	}
	
h1
{
    color:#e2e2e2;
    font-family: 'futura light condensed', Arial, Sans-Serif;
    font-size:36px;
    padding-top:10px;
    padding-bottom:30px;
    text-transform:uppercase;
    font-weight:normal;
}
	
h2
{
    color:#e2e2e2;
    font-family: 'futura light condensed', Arial, Sans-Serif;
    font-size:36px;
    font-weight:normal;
}

h3
{
    font-family:'futura medium condensed', Arial, Sans-Serif; 
    font-size:38px; 
    line-height:40px;
    color:#e2e2e2;
    font-weight:normal;
}

/*Light gray background */
.body_wrapper
{
    background-image: url('/images/lt_bg_texture.jpg'); 
    background-repeat:repeat-y; 
    background-position:top center; 
    overflow:auto;
}

.body_wrapper2
{
    background-image: url('/images/lt_bg_gradient.jpg'); 
    background-repeat:no-repeat; 
    background-position:top center; 
    overflow:auto;
}

/*dark gray background */
.dark_body_wrapper
{
    background-image: url('/images/dk_bg_texture.jpg'); 
    background-repeat:repeat-y; 
    background-position:top center; 
    overflow:auto;
    background-color:#1c1c1c;
}

.dark_body_wrapper2
{
    background-image: url('/images/dk_bg_gradient.jpg'); 
    background-repeat:no-repeat; 
    background-position:top center; 
    overflow:auto;
}

.red_h2 /* fryer */
{
    color:#CA2125;
}

.purple_h2 /* ovens */
{
    color:#B612B6;
}

.orange_h2 /* griddle & charbroiler */
{
    color:#CF4025;
}

.green_h2 /* braising pan, kettles, steamers*/
{
    color:#379f2a;
}

.aqua_h2 /* Heated Holding */
{
    color:#09AA8C;
}

.blue_h2
{
     color:#4793ea;
}

.yellow_h2 {
	color:#f4c962;
}

.irx_h3
{
    color:#fefcfc;
    font-family: 'helvetica', Arial, Sans-Serif;
    font-size:23px;
    line-height:25px;
    padding-bottom:12px;
}


p, li
{
    color:#d4d3d3;
    font-family: 'helvetica', Arial, Sans-Serif;
    font-size:16px;
    line-height:22px;
    padding-bottom:14px;
}


a
{
    color:#5cb0eb;
    text-decoration:none;
}

.scrollup
{
    width:75px;
    height:75px;
    position:fixed;
    bottom:100px;
    right:10%;
    margin-right: -45px;
    display:none;
    text-indent:-9999px;
    background: url('/images/top.png') no-repeat;
    z-index:100000;
}
	
.row {
      width: 100%;
      margin-left: auto;
      margin-right: auto;
      margin-top: 0;
      margin-bottom: 0;
      max-width: 1055px;
      *zoom: 1; 
    
}	

.row_padding
{
    padding: 0 15px;
}


.twocolumn {
      position: relative;
      padding-left: 0em;
      padding-right: 0em;
      float: left;
      width: 50%;
  }
  
 .fourcolumn
 {
     width:25%;
     float:left;
     position:relative;
 }
 
 .fivecolumn {
	 width:20%;
     float:left;
     position:relative;
 }
 
 .menu_fourcolumn
 {
     width:25%;
     float:left;
     position:relative;
 }
 
 .threecolumn
 {
     width:30%;
     padding-right:3%;
     float:left;
     position:relative;
 }
	  
.sixty {
          position: relative;
          padding-left: 0em;
          padding-right: 0em;
          float: left;
          width: 60%;
      }
	  
.forty {
          position: relative;
          padding-left: 0em;
          padding-right: 0em;
          float: left;
          width: 40%;
      }
      
.dots_background
{
    background-image: url('/images/dots_background.jpg'); 
    background-repeat:no-repeat;
    background-position:center bottom;
    background-color:#2c2c2c;
}

.dots_background_top
{
    background-image: url('/images/dots_background_top.jpg'); 
    background-repeat:no-repeat;
    background-position:center top;
    background-color:#272727;
	overflow:auto;
}

#header_background
{
	width:100%;
	height:113px;
	background-image:url(/images/nav_background.jpg);
	background-repeat:repeat-x;
	border-bottom: 2px solid #5d5d5d;
	position:fixed; 
	top:0px; 
	height:113px; 
	z-index:8000;
}
	
#header
{
	width:100%;
	max-width:1055px;
	position:relative;
	margin:0 auto;
}

#search_bar, #search_bar a{
	position:relative;
	float:right;
	font-size:11px;
	font-family:'helvetica', arial, sans-serif;
	color:#acacac;
	text-decoration:none;
}

.body-top-padding
{
    position:relative; 
    top:115px;
}

#content
{
	width:100%;
	max-width:1055px;
	margin: 0 auto;
}

.header_copy p
{
    font-family:'helvetica', arial, sans-serif;
    font-size: 16px;
    line-height: 24px;
    color:#d4d3d3;
    text-align:left;
}

#homepage_slider
{
	width:100%;
	max-width:910px;
	margin: 0 auto;
	border-bottom: 2px solid #5d5d5d;
}

/*  Header Navigation  */

    .nav_main{
	    float:right;
	    width:100%;
	    max-width:1055px;
	    padding-top:10px;
    }
    
    .nav-link {
	    width:20%;
	    min-width:100px;
	    text-align:center;
	    font-family: 'futura light condensed', Arial, Sans-Serif; 
	    font-size: 24px;
	    color:#ebebeb;
	    vertical-align:top;
	    float:left;
    }
    
    .nav-link  a
    {
	    font-family: 'futura light condensed', Arial, Sans-Serif; 
	    font-size: 24px;
	    color:#ebebeb;
	    text-decoration: none;
    }
    
    
/* Footer */

    .footer_square
    {
        background-image: url('/images/footer_square.png'); 
        width:413px; 
        height:236px; 
        margin: 0 auto;
    }
    
    #footer_squares
    {
        background-color:#191919; 
        background-image: url('/images/footer_bg.jpg'); 
        background-position:bottom center; 
        background-repeat:no-repeat;
        width:100%;
        min-height:397px; 
        height:auto; 
        overflow:auto; 
    }
    
    .foooter_header 
    {
        font-family: 'futura medium', Arial, Sans-Serif;
        font-size:20px;
        color:#909090;
        position:relative;
        text-align:center;
        width:100%;
        top:30px;
    }
    
    .footer_tools
    {
        position:relative;
        top:70px;
    }
    
    .footer_tools a
    {
        font-family:'futura', Arial, Sans-Serif;
        font-size:13px;
        color:#ffffff;
        text-decoration:none;
    }
    
    #brands
    {
        position:relative; 
        top:50px; 
        width:100%; 
        max-width:342px;
    }


    @media all and (max-width:890px) 
	{
		#footer_squares .twocolumn
        {
            width:100%; 
        }
        
        .footer_square
        {
            margin-bottom:15px;
        }
	}
	
	@media all and (max-width:460px) 
	{
		.footer_square
        {
            background-image: url('/images/footer_square_sm.png'); 
            width:310px; 
            height:177px; 
        }
        
        .foooter_header 
        {
            top:8px;
            font-size:18px;
        }
        
        #footer_tech
        {
            top:25px;
        }
        
        #brands
        {
            top:0px; 
            width:80%; 
        }
        
        .footer_tools
        {
            top:40px;
        }
        .footer_tools a
        {
            font-size:11px;
        }
        
        .footer_tools td[style]
        {
            padding-bottom:15px;
        }
	}
    

/*Homepage */

    .video_heading1 {
	    color:#d9d9d9;
	    font-size:24px;
	    font-family:'futura medium condensed', Arial, sans-serif;
	    line-height:28px;
	    padding-bottom:10px;
    }

    .video_heading2 {
	    color:#9f9e9e;
	    font-size:16px;
	    font-family:'helvetica', Arial, sans-serif;
    }
    
    .feature_break, .feature_break2
    {
        width:100%;
        float:left;
    }
    
    .responsive-container { position: relative; padding-bottom: 56.25%; padding-top: 30px; height: 0; overflow: hidden; }
    .responsive-container iframe { position: absolute; top: 0; left: 0; width: 100%; height: 100%; }
    
    .video_padding
    {
        padding:40px; 
        background-color:#222222;
    }
    
    @media all and (max-width:640px) 
    {
        .video_padding
        {
            padding:20px 10px; 
        }
    }

    @media all and (max-width:500px) 
    {
        .video_heading1 {font-size:20px; line-height:24px;}
        .video_heading2 {display:none; height:1px; line-height:1px;}
    }


/* Homepage Center Slider */

    .content-slide {
      color: #fff;
    }

    #home_gutter
    {
	    width:70px;
    }

